{
  "compilerOptions": {
    "allowJs": true,
    "module": "umd",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "es6",
      "es2020",
      "dom",
      "dom.iterable"
    ],
    "removeComments": true,
    "target": "ES2020",
    "noImplicitAny": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "jsx": "react"
  }
}
